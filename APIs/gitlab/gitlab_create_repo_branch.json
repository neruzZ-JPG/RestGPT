{
    "https://gitlab.com/api/v4/projects/{id}/repository/branches": {
        "post": {
            "description": "Function: Create a branch in a project's repository. Inputs: Project ID, branch name, and ref (branch/commit SHA). Response: On success (200), returns details like commit info, branch status etc. in JSON.",
            "tags": [
                "branch"
            ],
            "parameters": [
                {
                    "name": "id",
                    "schema": {
                        "type": "integer"
                    },
                    "required": true,
                    "in": "path",
                    "description": "ID or URL-encoded path of the project owned by the authenticated user."
                },
                {
                    "name": "branch",
                    "schema": {
                        "type": "string"
                    },
                    "required": true,
                    "in": "query",
                    "description": "Name of the branch."
                },
                {
                    "name": "ref",
                    "schema": {
                        "type": "string"
                    },
                    "required": true,
                    "in": "query",
                    "description": "Branch name or commit SHA to create branch from."
                }
            ],
            "responses": {
                "200": {
                    "description": "success",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "commit": {
                                        "type": "object",
                                        "properties": {
                                            "author_email": {
                                                "type": "string"
                                            },
                                            "author_name": {
                                                "type": "string"
                                            },
                                            "authored_date": {
                                                "type": "string"
                                            },
                                            "committed_date": {
                                                "type": "string"
                                            },
                                            "committer_email": {
                                                "type": "string"
                                            },
                                            "committer_name": {
                                                "type": "string"
                                            },
                                            "id": {
                                                "type": "string"
                                            },
                                            "short_id": {
                                                "type": "string"
                                            },
                                            "title": {
                                                "type": "string"
                                            },
                                            "message": {
                                                "type": "string"
                                            },
                                            "parent_ids": {
                                                "type": "array",
                                                "items": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    },
                                    "name": {
                                        "type": "string"
                                    },
                                    "merged": {
                                        "type": "boolean"
                                    },
                                    "protected": {
                                        "type": "boolean"
                                    },
                                    "default": {
                                        "type": "boolean"
                                    },
                                    "developers_can_push": {
                                        "type": "boolean"
                                    },
                                    "developers_can_merge": {
                                        "type": "boolean"
                                    },
                                    "can_push": {
                                        "type": "boolean"
                                    },
                                    "web_url": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}