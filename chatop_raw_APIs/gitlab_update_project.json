                                },
                                "visibility": {
                                    "type": "string",
                                    "enum": [
                                        "private",
                                        "internal",
                                        "public"
                                    ],
                                    "description": "See project visibility level."
                                },
                                "wiki_access_level": {
                                    "type": "string",
                                    "description": "One of disabled, private, or enabled."
                                },
                                "wiki_enabled": {
                                    "type": "boolean",
                                    "description": "(Deprecated) Enable wiki for this project. Use wiki_access_level instead."
                                }
