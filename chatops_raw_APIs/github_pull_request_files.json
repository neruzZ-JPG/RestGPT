                                                },
                                                "field": {
                                                    "type": "string"
                                                },
                                                "message": {
                                                    "type": "string"
                                                },
                                                "code": {
                                                    "type": "string"
                                                },
                                                "index": {
                                                    "type": "integer"
                                                },
                                                "value": {
                                                    "oneOf": [
                                                        {
                                                            "type": "string",
                                                            "nullable": true
                                                        },
                                                        {
                                                            "type": "integer",
                                                            "nullable": true
                                                        },
                                                        {
                                                            "type": "array",
                                                            "nullable": true,
                                                            "items": {
                                                                "type": "string"
                                                            }
                                                        }
                                                    ]
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
