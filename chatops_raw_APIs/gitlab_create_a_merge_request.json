{
    "openapi": "3.0.0",
    "info": {
        "title": "Create Merge Request API",
        "version": "1.0.0",
        "description": "The Create Merge Request API enables the creation of a new merge request. It requires details like project ID, source and target branches, title, etc. Optional parameters include allowing collaboration, setting approvals before merge, assigning users, adding descriptions, labels, and more. It returns comprehensive information about the created merge request such as its state, author, assignee, labels, and related project and milestone details."
    },
    "paths": {
        "https://gitlab.com/api/v4/projects/{id}/merge_requests": {
            "post": {
                "operationId": "createMergeRequest",
                "summary": "Creates a new merge request",
                "description": "This endpoint creates a new merge request with the provided details.",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "The ID or URL-encoded path of the project. Required.",
                        "required": true,
                        "schema": {
                            "oneOf": [
                                {
                                    "type": "integer"
                                },
                                {
                                    "type": "string"
                                }
                            ]
                        }
                    }
                ],
                "requestBody": {
                    "description": "The details of the merge request to be created.",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "source_branch": {
                                        "type": "string",
                                        "description": "The source branch. Required."
                                    },
                                    "target_branch": {
                                        "type": "string",
                                        "description": "The target branch. Required."
                                    },
                                    "title": {
                                        "type": "string",
                                        "description": "Title of MR. Required."
                                    },
                                    "allow_collaboration": {
                                        "type": "boolean",
                                        "description": "Allow commits from members who can merge to the target branch. Optional."
                                    },
                                    "approvals_before_merge": {
                                        "type": "integer",
                                        "description": "Number of approvals required before this merge request can merge. Deprecated in GitLab 16.0. Premium and Ultimate only. Optional."
                                    },
                                    "allow_maintainer_to_push": {
                                        "type": "boolean",
                                        "description": "Alias of allow_collaboration. Optional."
                                    },
                                    "assignee_id": {
                                        "type": "integer",
                                        "description": "Assignee user ID. Optional."
                                    },
                                    "assignee_ids": {
                                        "type": "array",
                                        "items": {
                                            "type": "integer"
                                        },
                                        "description": "The ID of the users to assign the merge request to. Set to 0 or provide an empty value to unassign all assignees. Optional."
                                    },
                                    "description": {
                                        "type": "string",
                                        "description": "Description of the merge request. Limited to 1,048,576 characters. Optional."
                                    },
                                    "labels": {
                                        "type": "string",
                                        "description": "Labels for the merge request, as a comma-separated list. If a label does not already exist, this creates a new project label and assigns it to the merge request. Optional."
                                    },
                                    "milestone_id": {
                                        "type": "integer",
                                        "description": "The global ID of a milestone. Optional."
                                    },
                                    "remove_source_branch": {
                                        "type": "boolean",
                                        "description": "Flag indicating if a merge request should remove the source branch when merging. Optional."
                                    },
                                    "reviewer_ids": {
                                        "type": "array",
                                        "items": {
                                            "type": "integer"
                                        },
                                        "description": "The ID of the users added as a reviewer to the merge request. If set to 0 or left empty, no reviewers are added. Optional."
                                    },
                                    "squash": {
                                        "type": "boolean",
                                        "description": "If true, squash all commits into a single commit on merge. Project settings might override this value. Optional."
                                    },
                                    "target_project_id": {
                                        "type": "integer",
                                        "description": "Numeric ID of the target project. Optional."
                                    }
                                },
                                "required": [
                                    "source_branch",
                                    "target_branch",
                                    "title"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful creation of the merge request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "id": {
                                            "type": "integer"
                                        },
