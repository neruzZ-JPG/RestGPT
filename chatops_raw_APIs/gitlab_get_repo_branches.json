{
    "https://gitlab.com/api/v4/projects/{id}/repository/branches": {
        "get": {
            "description": "Function: Get project repository branches. Input: 'id' (project ID/URL-encoded path) required, 'search' for filtering branches. Response: 200 with branch details like name, merge status, etc. in JSON. Returns info on branches of authenticated user's project.",
            "tags": [
                "branches"
            ],
            "parameters": [
                {
                    "name": "id",
                    "schema": {
                        "oneOf": [
                            {
                                "type": "integer"
                            },
                            {
                                "type": "string"
                            }
                        ]
                    },
                    "required": true,
                    "in": "path",
                    "description": "ID or URL-encoded path of the project owned by the authenticated user."
                },
                {
                    "name": "search",
                    "schema": {
                        "type": "string"
                    },
                    "required": false,
                    "in": "query",
                    "description": "Return list of branches containing the search string. You can use ^term and term$ to find branches that begin and end with term respectively."
                }
            ],
            "responses": {
                "200": {
                    "description": "success",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "merged": {
                                        "type": "boolean"
                                    },
                                    "protected": {
                                        "type": "boolean"
                                    },
                                    "default": {
                                        "type": "boolean"
                                    },
                                    "developers_can_push": {
                                        "type": "boolean"
                                    },
                                    "developers_can_merge": {
                                        "type": "boolean"
                                    },
                                    "can_push": {
                                        "type": "boolean"
                                    },
                                    "web_url": {
                                        "type": "string"
                                    },
                                    "commit": {
                                        "type": "object",
                                        "properties": {
                                            "author_email": {
                                                "type": "string"
                                            },
                                            "author_name": {
                                                "type": "string"
                                            },
                                            "authored_date": {
                                                "type": "string"
                                            },
                                            "committed_date": {
                                                "type": "string"
                                            },
                                            "committer_email": {
                                                "type": "string"
                                            },
                                            "committer_name": {
                                                "type": "string"
                                            },
                                            "id": {
                                                "type": "string"
                                            },
                                            "short_id": {
                                                "type": "string"
                                            },
                                            "title": {
                                                "type": "string"
                                            },
                                            "message": {
                                                "type": "string"
                                            },
                                            "parent_ids": {
                                                "type": "array",
                                                "items": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}