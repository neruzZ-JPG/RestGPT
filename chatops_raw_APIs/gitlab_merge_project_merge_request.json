                                                },
                                                "avatar_url": {
                                                    "type": "string",
                                                    "nullable": true
                                                },
                                                "web_url": {
                                                    "type": "string"
                                                }
                                            }
                                        },
                                        "assignee": {
                                            "type": "object",
                                            "properties": {
                                                "id": {
                                                    "type": "integer"
                                                },
                                                "name": {
                                                    "type": "string"
                                                },
                                                "username": {
                                                    "type": "string"
                                                },
                                                "state": {
                                                    "type": "string"
                                                },
                                                "avatar_url": {
