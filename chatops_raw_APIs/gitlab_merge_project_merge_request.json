{
    "openapi": "3.0.0",
    "info": {
        "title": "MergeRequestMerger API",
        "version": "1.0.0",
        "description": "The MergeRequestMerger API enables the acceptance and merging of changes submitted via merge requests. It supports various attributes to customize the merge operation. It also returns specific HTTP status codes on failure and provides detailed response data about the merge request."
    },
    "paths": {
        "https://gitlab.com/api/v4/projects/{id}/merge_requests/{merge_request_iid}/merge": {
            "put": {
                "operationId": "mergeMergeRequest",
                "description": "Merge a specific merge request with optional customizations.",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Required. Integer or string. The ID or URL-encoded path of the project.",
                        "required": true,
                        "schema": {
                            "oneOf": [
                                {
                                    "type": "integer"
                                },
                                {
                                    "type": "string"
                                }
                            ]
                        }
                    },
                    {
                        "name": "merge_request_iid",
                        "in": "path",
                        "description": "Required. Integer. The internal ID of the merge request.",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "merge_commit_message",
                        "in": "query",
                        "description": "Optional. String. Custom merge commit message.",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "merge_when_pipeline_succeeds",
                        "in": "query",
                        "description": "Optional. Boolean. If true, the merge request merges when the pipeline succeeds.",
                        "required": false,
                        "schema": {
                            "type": "boolean"
                        }
                    },
                    {
                        "name": "sha",
                        "in": "query",
                        "description": "Optional. String. If present, then this SHA must match the HEAD of the source branch, otherwise the merge fails.",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "should_remove_source_branch",
                        "in": "query",
                        "description": "Optional. Boolean. If true, removes the source branch.",
                        "required": false,
                        "schema": {
                            "type": "boolean"
                        }
                    },
                    {
                        "name": "squash_commit_message",
                        "in": "query",
                        "description": "Optional. String. Custom squash commit message.",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "squash",
                        "in": "query",
                        "description": "Optional. Boolean. If true, squash all commits into a single commit on merge.",
                        "required": false,
                        "schema": {
                            "type": "boolean"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful merge request operation. Returns an object with detailed information about the merged merge request.",
